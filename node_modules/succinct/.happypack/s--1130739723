import { IDENTITY } from '../commons/constant';
// import Succinc from '../succinct';
// import Context from '../context';
export default function action(value, key, descriptor) {
    var des = {
        configurable: true,
        enumerable: descriptor.enumerable,
        get: function get() {
            return this[IDENTITY].get(key);
        },
        set: function set(newValue) {
            this[IDENTITY].action(newValue);
        }
    };
    return des;
}